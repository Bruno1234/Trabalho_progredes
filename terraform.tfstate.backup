            "rendered": "#cloud-config\nhostname: dhcp-server\nusers:\n  - name: aluno\n    plain_text_passwd: \"1234\"\n    lock_passwd: false\n    sudo: [\"ALL=(ALL) NOPASSWD:ALL\"]\n    shell: /bin/bash\n\nwrite_files:\n  - path: /etc/dhcp/dhcpd.conf\n    permissions: '0644'\n    content: |\n      subnet 192.168.122.0 netmask 255.255.255.0 {\n        range 192.168.122.100 192.168.122.200;\n        option routers 192.168.122.1;\n        option domain-name-servers 8.8.8.8;\n        default-lease-time 600;\n        max-lease-time 7200;\n      }\n\n  - path: /usr/local/bin/setup-dhcp.sh\n    permissions: '0755'\n    content: |\n      #!/bin/bash\n      apt-get update\n      apt-get install -y isc-dhcp-server net-tools\n\n      iface=$(ip link | awk -F: '$0 !~ \"lo|virbr|wl|^[^0-9]\"{print $2;getline}' | head -n 1 | tr -d ' ')\n      echo \"INTERFACESv4=\\\"$iface\\\"\" \u003e /etc/default/isc-dhcp-server\n\n      systemctl enable isc-dhcp-server\n      systemctl restart isc-dhcp-server\n\nruncmd:\n  - bash /usr/local/bin/setup-dhcp.sh\n",
